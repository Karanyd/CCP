  #include<stdio.h>
            #include<conio.h>
            int main()
            {
            float celsius,fahrenheit;
            clrscr();
            printf(" Enter the temperature in fahrenheit:");
            scanf("%f",&fahrenheit);
            celsius=0.56*(fahrenheit-32);
            printf("%f fahrenheit =%f celsius",fahrenheit,celsius);
            getch();
            return(0);
            }
